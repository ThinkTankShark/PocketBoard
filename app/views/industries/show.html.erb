


<!-- start padding container -->
    <div class="wrap container">
        <!-- start jtinder container -->
        <div id="tinderslide">
            <ul>
            <% @stocks.each do |stock| %>
             <% stock.id %>
                <li class="pane1">
                    <div class="img"></div>
                    <div><p><strong>Name: </strong><%= stock.name %></p><p><strong>Symbol: </strong><%= stock.symbol %></p></div>
                    <div class="like"></div>
                    <div class="dislike"></div>
                </li>
                <% end %>
            </ul>
        </div>
        <!-- end jtinder container -->
    </div>
    <!-- end padding container -->

    <!-- jTinder trigger by buttons  -->
    <div class="actions">
        <a href="#" class="dislike"><i></i></a>
        <a href="#" class="like"><i></i></a>
    </div>

    <!-- jTinder status text  -->
    <div id="status"></div>

    <!-- jQuery lib -->
    <script type="text/javascript" src="/assets/jquery.min.js"></script>
    <!-- transform2d lib -->
    <script type="text/javascript" src="/assets/jquery.transform2d.js"></script>
    <!-- jTinder lib -->
    <script type="text/javascript" src="/assets/jquery.jTinder.js"></script>
    <!-- jTinder initialization script -->
    <script type="text/javascript" src="/assets/main.js"></script>
